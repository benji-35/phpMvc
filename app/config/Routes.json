{
  "controllers": {
    "ExampleController": {
      "namespace": "App\\Controller",
      "location": "example/ExampleController.php"
    }
  },
  "urls": {
    "/": {
      "GET": {
        "controller": "ExampleController",
        "function": "index"
      }
    },
    "/createUser": {
      "POST": {
        "controller": "ExampleController",
        "function": "createUser"
      }
    },
    "/loginUser": {
      "POST": {
        "controller": "ExampleController",
        "function": "loginUser"
      }
    },
    "/login": {
      "GET": {
        "controller": "ExampleController",
        "function": "login"
      }
    },
    "/register": {
      "GET": {
        "controller": "ExampleController",
        "function": "register"
      }
    },
    "/logout": {
      "POST": {
        "controller": "ExampleController",
        "function": "logout"
      }
    }
  },
  "not_found": {
    "layout": ""
  }
}
