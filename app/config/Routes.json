{
  "controllers": {
    "ExampleController": {
      "namespace": "App\\Controller",
      "location": "example/ExampleController.php"
    }
  },
  "urls": {
    "/": {
      "GET": {
        "controller": "ExampleController",
        "function": "index"
      }
    },
    "/createUser": {
      "POST": {
        "controller": "ExampleController",
        "function": "createUser"
      }
    }
  },
  "not_found": {
    "layout": ""
  }
}
